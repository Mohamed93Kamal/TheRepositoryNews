@model List<ContentChangeLogViewModel>

@{
    Layout = null;
            }

<table class="table table-hover">
    <tr class="alert alert-success">
        <th>الوقت</th>
        <th>من</th>
        <th>الى</th>
    </tr>
    @foreach (var post in Model)
    {
        <tr>
            <th>@post.ChangeAt</th>
            <th>@post.Old</th>
            <th>@post.New</th>
        </tr>
    }
</table>